$NOMOD51
NAME    ?C_STARTUP

EXTRN CODE (?C_START)
PUBLIC  ?C_STARTUP

CLKCON       EQU		0B2H
	

EXTRN	IDATA	(STACK)

                CSEG    AT      0
?C_STARTUP:     LJMP    STARTUP1



?C_C51STARTUP		SEGMENT	CODE
								RSEG			?C_C51STARTUP
STARTUP1:
                MOV     SP, #STACK-1
				MOV		CLKCON, #08H	;SETB HFON
				NOP
				NOP
				NOP
				NOP
				NOP
				NOP
				NOP
				NOP
				ORL		CLKCON, #04H		;SETB FS, SYSCLK=24M
				LJMP    ?C_START

                END